<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Frame2_2">

    <TextView
            android:text="@string/parsonal_title"
            android:textSize="25sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/textView11" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"/>
    <TextView
            android:text="@string/Birthday"
            android:layout_width="60dp"
            android:layout_height="20dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp" android:id="@+id/birthday_title"
            app:layout_constraintTop_toBottomOf="@+id/textView11" android:layout_marginTop="36dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.172"/>
    <EditText
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/Birthday_form"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/birthday_title"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"/>
    <TextView
            android:text="@string/weight"
            android:layout_width="60dp"
            android:layout_height="20dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp" android:id="@+id/weight_title"
            app:layout_constraintTop_toBottomOf="@+id/Birthday_form" android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.172"/>
    <EditText
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/weight_form"
            app:layout_constraintTop_toBottomOf="@+id/weight_title" android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"/>
    <TextView
            android:text="@string/hight"
            android:layout_width="60dp"
            android:layout_height="20dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp"
            android:id="@+id/hight_title" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/weight_form" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.156"/>
    <EditText
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/hight_form"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/hight_title"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"/>
    <TextView
            android:text="@string/sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/sex_title" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/hight_form" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="36dp" android:layout_marginStart="36dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.156"/>
    <Button
            android:text="@string/Make_account"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:id="@+id/Make_acount" android:layout_marginTop="488dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>
    <RadioGroup
            android:layout_width="71dp"
            android:layout_height="60dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/sex_title" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp">
        <RadioButton
                android:text="@string/sex_male"
                android:layout_width="70dp"
                android:layout_height="20dp" android:id="@+id/radio_man" android:layout_weight="1"/>
        <RadioButton
                android:text="@string/sex_female"
                android:layout_width="70dp"
                android:layout_height="20dp" android:id="@+id/radio_woman" android:layout_weight="1"/>
    </RadioGroup>
</androidx.constraintlayout.widget.ConstraintLayout>